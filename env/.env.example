# 需要手动配置以下配置
# .env.development
# .env.staging
# .env.production

# 应用配置
NODE_ENV=development
APP_NAME=rsbuild-monorepo-demo
APP_VERSION=1.0.0

# 前端配置
CLIENT_PORT=3000
CLIENT_HOST=localhost
PUBLIC_URL=/

# 构建配置
BUILD_ANALYZE=false
BUILD_SOURCEMAP=true
BUILD_MINIFY=false

# 开发工具
HOT_RELOAD=true
OPEN_BROWSER=true

# 性能监控
ENABLE_PERF_MONITOR=false

# UnoCSS 配置
UNOCSS_ENABLE=true
